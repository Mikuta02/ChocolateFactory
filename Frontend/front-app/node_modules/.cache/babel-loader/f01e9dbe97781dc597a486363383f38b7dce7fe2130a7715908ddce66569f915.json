,"names":["key","class","_createElementVNode","$setup","factory","_createElementBlock","_hoisted_1","_hoisted_2","_toDisplayString","name","src","getFactoryLogoUrl","logoPath","alt","_hoisted_3","workingHours","status","location","address","latitude","longitude","rating","_hoisted_4","chocolates","length","_hoisted_5","_Fragment","_renderList","chocolate","id","onClick","$event","editChocolate","_hoisted_6","userRole","editAmount","_hoisted_7","_createCommentVNode","confirmDelete","_hoisted_8","getChocolatePictureUrl","picturePath","_hoisted_9","_hoisted_10","_hoisted_11","chocolateType","chocolateVariety","price","grams","_hoisted_12","description","amount","addToCart","_hoisted_13","_hoisted_14","addNewChocolate","goToAddComment","_createVNode","factoryId","showModal","_hoisted_15","_hoisted_16","deleteChocolate","cancelDelete"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\FactoryDetails.vue"],"sourcesContent":["<template>\r\n  <div class=\"factory-details\" v-if=\"factory\">\r\n    <h1>DETAILED VIEW</h1>\r\n    <h2>{{ factory.name }}</h2>\r\n    <img :src=\"getFactoryLogoUrl(factory.logoPath)\" :alt=\"factory.name + ' logo'\" class=\"factory-logo\" />\r\n    <p>Working Hours: {{ factory.workingHours }}</p>\r\n    <p>Status: {{ factory.status }}</p>\r\n    <p>Location: {{ factory.location.address }}</p>\r\n    <p>Latitude: {{ factory.location.latitude }}</p>\r\n    <p>Longitude: {{ factory.location.longitude }}</p>\r\n    <p>Rating: {{ factory.rating }}</p>\r\n\r\n    <h2>Chocolates</h2>\r\n    <ul v-if=\"chocolates.length\">\r\n      <li v-for=\"chocolate in chocolates\" :key=\"chocolate.id\" class=\"chocolate-item\">\r\n        <button @click=\"editChocolate(chocolate.id)\" class=\"edit-button\">Edit</button>\r\n        <button v-if=\"userRole === 'Worker'\" @click=\"editAmount(chocolate.id)\" class=\"edit-amount-button\">Edit Amount</button>\r\n        <button @click=\"confirmDelete(chocolate.id)\" class=\"delete-button\">X</button>\r\n        <img :src=\"getChocolatePictureUrl(chocolate.picturePath)\" :alt=\"chocolate.name + ' picture'\" class=\"chocolate-picture\" />\r\n        <div class=\"chocolate-details\">\r\n          <h3 class=\"chocolate-name\">{{ chocolate.name }}</h3>\r\n          <p>Type: {{ chocolate.chocolateType }}</p>\r\n          <p>Variety: {{ chocolate.chocolateVariety }}</p>\r\n          <p>Price: {{ chocolate.price }}</p>\r\n          <p>Grams: {{ chocolate.grams }}</p>\r\n          <p class=\"chocolate-description\">Description: {{ chocolate.description }}</p>\r\n          <p>Status: {{ chocolate.status }}</p>\r\n          <p>Amount: {{ chocolate.amount }}</p>\r\n          <button @click=\"addToCart(chocolate.id, 1)\" class=\"add-to-cart-button\">Add to Cart</button>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <p v-else>No chocolates available for this factory.</p>\r\n\r\n    <button @click=\"addNewChocolate\" class=\"add-button\">Add New Chocolate</button>\r\n    <button @click=\"goToAddComment\" class=\"add-comment-button\">Add Comment</button> <!-- Dodato dugme za dodavanje komentara -->\r\n    <Comments :factoryId=\"factory.id\" /> <!-- Dodato prikazivanje komentara -->\r\n\r\n    <div v-if=\"showModal\" class=\"modal-overlay\">\r\n      <div class=\"modal\">\r\n        <p>Are you sure you want to remove this chocolate from the system?</p>\r\n        <button @click=\"deleteChocolate\" class=\"confirm-button\">Yes</button>\r\n        <button @click=\"cancelDelete\" class=\"cancel-button\">No</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport axios from 'axios';\r\nimport { jwtDecode } from 'jwt-decode'; \r\nimport Comments from './Comments.vue'; \r\n\r\nconst route = useRoute();\r\nconst router = useRouter();\r\nconst factory = ref(null);\r\nconst chocolates = ref([]);\r\nconst loadingChocolates = ref(false);\r\nconst showModal = ref(false);\r\nconst chocolateToDelete = ref(null);\r\nconst userRole = ref(null); // Dodato polje za ulogu korisnika\r\n\r\nonMounted(() => {\r\n  loadFactory();\r\n  decodeToken(); // Dekodiranje tokena prilikom montiranja komponente\r\n});\r\n\r\nfunction loadFactory() {\r\n  const factoryId = route.params.id;\r\n  axios.get(`http://localhost:3001/api/factories/${factoryId}`)\r\n    .then(response => {\r\n      factory.value = response.data;\r\n      loadChocolates(factoryId);\r\n    })\r\n    .catch(error => {\r\n      console.error('Error fetching factory details:', error);\r\n    });\r\n}\r\n\r\nfunction loadChocolates(factoryId) {\r\n  loadingChocolates.value = true;\r\n  axios.get(`http://localhost:3001/api/chocolates?factoryId=${factoryId}`)\r\n    .then(response => {\r\n      chocolates.value = response.data;\r\n      loadingChocolates.value = false;\r\n    })\r\n    .catch(error => {\r\n      console.error('Error fetching chocolates:', error);\r\n      loadingChocolates.value = false;\r\n    });\r\n}\r\n\r\nfunction decodeToken() {\r\n  const token = localStorage.getItem('token'); // Pretpostavimo da je token sačuvan u localStorage\r\n  if (token) {\r\n    const decoded = jwtDecode(token);\r\n    console.log(decoded); // Provera sadržaja tokena\r\n    userRole.value = decoded.role;\r\n    console.log('User role:', userRole.value); // Provera vrednosti userRole\r\n  }\r\n}\r\n\r\nfunction addNewChocolate() {\r\n  const factoryId = route.params.id;\r\n  router.push({ name: 'AddChocolate', query: { factoryId } });\r\n}\r\n\r\nfunction editChocolate(chocolateId) {\r\n  router.push({ name: 'EditChocolate', params: { factoryId: factory.value.id, chocolateId } });\r\n}\r\n\r\nfunction confirmDelete(id) {\r\n  chocolateToDelete.value = id;\r\n  showModal.value = true;\r\n}\r\n\r\nfunction deleteChocolate() {\r\n  axios.delete(`http://localhost:3001/api/chocolates/${chocolateToDelete.value}`)\r\n    .then(() => {\r\n      chocolates.value = chocolates.value.filter(chocolate => chocolate.id !== chocolateToDelete.value);\r\n      showModal.value = false;\r\n      chocolateToDelete.value = null;\r\n    })\r\n    .catch(error => {\r\n      console.error('Error deleting chocolate:', error);\r\n    });\r\n}\r\n\r\nfunction cancelDelete() {\r\n  showModal.value = false;\r\n  chocolateToDelete.value = null;\r\n}\r\n\r\nfunction addToCart(chocolateId, quantity) {\r\n  axios.post('http://localhost:3001/api/cart/add', { chocolateId, quantity })\r\n    .then(response => {\r\n      console.log('Added to cart:', response.data);\r\n    })\r\n    .catch(error => {\r\n      console.error('Error adding to cart:', error);\r\n    });\r\n}\r\n\r\nfunction getFactoryLogoUrl(path) {\r\n  return `http://localhost:3001/images/${path}`;\r\n}\r\n\r\nfunction getChocolatePictureUrl(path) {\r\n  return `http://localhost:3001/images/${path}`;\r\n}\r\n\r\nfunction goToAddComment() {\r\n  const factoryId = route.params.id;\r\n  router.push({ name: 'AddComment', params: { factoryId } });\r\n}\r\n\r\nfunction editAmount(chocolateId) {\r\n  const newAmount = prompt(\"Enter new amount:\");\r\n  if (newAmount !== null) {\r\n    axios.put(`http://localhost:3001/api/chocolates/${chocolateId}`, { amount: newAmount })\r\n      .then(response => {\r\n        const updatedChocolate = response.data;\r\n        const chocolateIndex = chocolates.value.findIndex(choc => choc.id === chocolateId);\r\n        if (chocolateIndex !== -1) {\r\n          chocolates.value[chocolateIndex].amount = updatedChocolate.amount;\r\n        }\r\n      })\r\n      .catch(error => {\r\n        console.error('Error updating amount:', error);\r\n      });\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.factory-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin: 20px;\r\n}\r\n.factory-logo {\r\n  width: 300px;\r\n  height: auto;\r\n  margin: 20px 0;\r\n}\r\n.chocolate-item {\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n  border: 1px solid #000;\r\n  padding: 10px;\r\n  border-radius: 8px;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  position: relative;\r\n}\r\n.chocolate-picture {\r\n  width: 100px;\r\n  height: auto;\r\n  margin-right: 20px;\r\n}\r\n.chocolate-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n  flex-grow: 1;\r\n}\r\n.chocolate-name {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n}\r\n.chocolate-description {\r\n  white-space: pre-line;\r\n}\r\n.delete-button, .edit-button, .edit-amount-button {\r\n  position: absolute;\r\n  top: 10px;\r\n  width: 30px;\r\n  height: 30px;\r\n  border: none;\r\n  border-radius: 50%;\r\n  color: white;\r\n  cursor: pointer;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n}\r\n.delete-button {\r\n  right: 45px;\r\n  background-color: red;\r\n}\r\n.edit-button {\r\n  right: 10px;\r\n  background-color: blue;\r\n}\r\n.edit-amount-button {\r\n  right: 80px; /* Adjust position to avoid overlap */\r\n  background-color: #B9770E; /* Custom color for the new button */\r\n}\r\n.add-button {\r\n  background-color: green;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 8px;\r\n  padding: 10px 20px;\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n}\r\n.add-to-cart-button {\r\n  background-color: orange;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 8px;\r\n  padding: 5px 10px;\r\n  cursor: pointer;\r\n  margin-top: 10px;\r\n}\r\n.add-comment-button {\r\n  background-color: blue;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 8px;\r\n  padding: 10px 20px;\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n}\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.modal {\r\n  background: white;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n}\r\n.confirm-button, .cancel-button {\r\n  margin: 10px;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n.confirm-button {\r\n  background-color: red;\r\n  color: white;\r\n}\r\n.cancel-button {\r\n  background-color: gray;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":";;;EAAAA,GAAA;EACOC,KAAK,EAAC;;gEACTC,mBAAA,CAAsB,YAAlB,eAAa;mBAFrB;gEAYIA,mBAAA,CAAmB,YAAf,YAAU;;EAZlBF,GAAA;AAAA;mBAAA;mBAAA;mBAAA;mBAAA;;EAmBaC,KAAK,EAAC;AAAmB;;EACxBA,KAAK,EAAC;AAAgB;;EAKvBA,KAAK,EAAC;AAAuB;oBAzB1C;;EAAAD,GAAA;AAAA;;EAAAA,GAAA;EAsC0BC,KAAK,EAAC;;iEAExBC,mBAAA,CAAsE,WAAnE,iEAA+D;;SAvCrCC,MAAA,CAAAC,OAAO,I,cAA1CC,mBAAA,CA4CM,OA5CNC,UA4CM,GA3CJC,UAAsB,EACtBL,mBAAA,CAA2B,YAAAM,gBAAA,CAApBL,MAAA,CAAAC,OAAO,CAACK,IAAI,kBACnBP,mBAAA,CAAqG;IAA/FQ,GAAG,EAAEP,MAAA,CAAAQ,iBAAiB,CAACR,MAAA,CAAAC,OAAO,CAACQ,QAAQ;IAAIC,GAAG,EAAEV,MAAA,CAAAC,OAAO,CAACK,IAAI;IAAYR,KAAK,EAAC;0BAJxFa,UAAA,GAKIZ,mBAAA,CAAgD,WAA7C,iBAAe,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACW,YAAY,kBACzCb,mBAAA,CAAmC,WAAhC,UAAQ,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACY,MAAM,kBAC5Bd,mBAAA,CAA+C,WAA5C,YAAU,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACa,QAAQ,CAACC,OAAO,kBACxChB,mBAAA,CAAgD,WAA7C,YAAU,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACa,QAAQ,CAACE,QAAQ,kBACzCjB,mBAAA,CAAkD,WAA/C,aAAW,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACa,QAAQ,CAACG,SAAS,kBAC3ClB,mBAAA,CAAmC,WAAhC,UAAQ,GAAAM,gBAAA,CAAGL,MAAA,CAAAC,OAAO,CAACiB,MAAM,kBAE5BC,UAAmB,EACTnB,MAAA,CAAAoB,UAAU,CAACC,MAAM,I,cAA3BnB,mBAAA,CAkBK,MA/BToB,UAAA,I,kBAcMpB,mBAAA,CAgBKqB,SAAA,QA9BXC,WAAA,CAc8BxB,MAAA,CAAAoB,UAAU,EAAvBK,SAAS;yBAApBvB,mBAAA,CAgBK;MAhBgCL,GAAG,EAAE4B,SAAS,CAACC,EAAE;MAAE5B,KAAK,EAAC;QAC5DC,mBAAA,CAA8E;MAArE4B,OAAK,EAAAC,MAAA,IAAE5B,MAAA,CAAA6B,aAAa,CAACJ,SAAS,CAACC,EAAE;MAAG5B,KAAK,EAAC;OAAc,MAAI,iBAf7EgC,UAAA,GAgBsB9B,MAAA,CAAA+B,QAAQ,iB,cAAtB7B,mBAAA,CAAsH;MAhB9HL,GAAA;MAgB8C8B,OAAK,EAAAC,MAAA,IAAE5B,MAAA,CAAAgC,UAAU,CAACP,SAAS,CAACC,EAAE;MAAG5B,KAAK,EAAC;OAAqB,aAAW,