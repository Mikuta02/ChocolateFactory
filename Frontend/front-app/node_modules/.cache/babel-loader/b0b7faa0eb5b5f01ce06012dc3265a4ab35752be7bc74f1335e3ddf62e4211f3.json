{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute();\n    const text = ref('');\n    const rating = ref(5);\n    const factoryId = ref(null);\n    onMounted(() => {\n      if (route.params.factoryId) {\n        factoryId.value = Number(route.params.factoryId);\n      }\n    });\n    const submitComment = () => {\n      const userId = store.getters.userId;\n      const token = store.state.token;\n      if (!userId) {\n        console.error('User is not defined or user.id is missing');\n        return;\n      }\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId: factoryId.value,\n        text: text.value,\n        rating: rating.value\n      }, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log('Comment added:', response.data);\n        router.push('/purchases');\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    };\n    return {\n      text,\n      rating,\n      submitComment\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useStore","axios","useRouter","useRoute","setup","store","router","route","text","rating","factoryId","params","value","Number","submitComment","userId","getters","token","state","console","error","post","headers","then","response","log","data","push","catch"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"text\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <input id=\"rating\" type=\"number\" v-model=\"rating\" min=\"1\" max=\"5\" required />\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport axios from 'axios';\r\nimport { useRouter, useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const text = ref('');\r\n    const rating = ref(5);\r\n    const factoryId = ref(null);\r\n\r\n    onMounted(() => {\r\n      if (route.params.factoryId) {\r\n        factoryId.value = Number(route.params.factoryId);\r\n      }\r\n    });\r\n\r\n    const submitComment = () => {\r\n      const userId = store.getters.userId;\r\n      const token = store.state.token;\r\n\r\n      if (!userId) {\r\n        console.error('User is not defined or user.id is missing');\r\n        return;\r\n      }\r\n\r\n      axios.post('http://localhost:3001/api/comments', {\r\n        userId,\r\n        factoryId: factoryId.value,\r\n        text: text.value,\r\n        rating: rating.value,\r\n      }, {\r\n        headers: {\r\n          'Authorization': `Bearer ${token}`\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Comment added:', response.data);\r\n        router.push('/purchases');\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding comment:', error);\r\n      });\r\n    };\r\n\r\n    return { text, rating, submitComment };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilizacija */\r\n</style>\r\n"],"mappings":";AAkBA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAEhD,eAAe;EACbC,KAAKA,CAAA