{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router'; // Dodajemo useRoute\n\nexport default {\n  data() {\n    return {\n      text: '',\n      rating: 5\n    };\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute(); // Dodajemo useRoute za pristup factoryId\n    return {\n      store,\n      router,\n      route\n    };\n  },\n  methods: {\n    submitComment() {\n      const userId = this.store.getters.userId;\n      const token = this.store.state.token;\n      const factoryId = this.route.params.factoryId; // Dobijamo factoryId iz rute\n\n      if (!userId) {\n        console.error('User is not defined or user.id is missing');\n        return;\n      }\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId,\n        text: this.text,\n        rating: this.rating\n      }, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log('Comment added:', response.data);\n        this.router.push('/purchases'); // Preusmeravanje na stranicu sa kupovinama\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["useStore","axios","useRouter","useRoute","data","text","rating","setup","store","router","route","methods","submitComment","userId","getters","token","state","factoryId","params","console","error","post","headers","then","response","log","push","catch"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"text\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <input id=\"rating\" type=\"number\" v-model=\"rating\" min=\"1\" max=\"5\" required />\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useStore } from 'vuex';\r\nimport axios from 'axios';\r\nimport { useRouter, useRoute } from 'vue-router'; // Dodajemo useRoute\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      text: '',\r\n      rating: 5,\r\n    };\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const route = useRoute(); // Dodajemo useRoute za pristup factoryId\r\n    return { store, router, route };\r\n  },\r\n  methods: {\r\n    submitComment() {\r\n      const userId = this.store.getters.userId;\r\n      const token = this.store.state.token;\r\n      const factoryId = this.route.params.factoryId; // Dobijamo factoryId iz rute\r\n\r\n      if (!userId) {\r\n        console.error('User is not defined or user.id is missing');\r\n        return;\r\n      }\r\n\r\n      axios.post('http://localhost:3001/api/comments', {\r\n        userId,\r\n        factoryId,\r\n        text: this.text,\r\n        rating: this.rating,\r\n      }, {\r\n        headers: {\r\n          'Authorization': `Bearer ${token}`\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Comment added:', response.data);\r\n        this.router.push('/purchases'); // Preusmeravanje na stranicu sa kupovinama\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding comment:', error);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilizacija */\r\n</style>\r\n"],"mappings":";AAkBA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY,EAAE;;AAElD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE;IACV,CAAC;EACH,CAA