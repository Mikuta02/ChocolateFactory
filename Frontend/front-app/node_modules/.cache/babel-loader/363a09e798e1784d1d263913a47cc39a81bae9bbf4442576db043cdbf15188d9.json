{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router'; // useRoute dodan\n\nexport default {\n  data() {\n    return {\n      text: '',\n      rating: 5,\n      factoryId: this.$route.params.factoryId // Koristimo factoryId iz rute\n    };\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute(); // useRoute dodan\n    return {\n      store,\n      router,\n      route\n    };\n  },\n  methods: {\n    submitComment() {\n      const userId = this.store.getters.userId;\n      const token = this.store.state.token;\n      if (!userId) {\n        console.error('User is not defined or user.id is missing');\n        return;\n      }\n      console.log(`Submitting comment with factoryId: ${this.factoryId}`); // Dodajemo log\n\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId: this.factoryId,\n        text: this.text,\n        rating: this.rating\n      }, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log('Comment added:', response.data);\n        this.router.push('/purchases');\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["useStore","axios","useRouter","useRoute","data","text","rating","factoryId","$route","params","setup","store","router","route","methods","submitComment","userId","getters","token","state","console","error","log","post","headers","then","response","push","catch"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n    