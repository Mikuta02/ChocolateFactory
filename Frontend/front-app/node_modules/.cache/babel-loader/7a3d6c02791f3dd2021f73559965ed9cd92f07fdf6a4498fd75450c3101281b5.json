{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, ref } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const isAuthenticated = computed(() => store.getters.isAuthenticated);\n    const showSideMenu = ref(false);\n    const toggleSideMenu = () => {\n      showSideMenu.value = !showSideMenu.value;\n      const sideMenuElement = document.querySelector('.side-menu');\n      if (showSideMenu.value) {\n        sideMenuElement.classList.add('show');\n      } else {\n        sideMenuElement.classList.remove('show');\n      }\n    };\n    const logout = () => {\n      store.dispatch('logout');\n      router.push('/login');\n      toggleSideMenu();\n    };\n    return {\n      isAuthenticated,\n      showSideMenu,\n      toggleSideMenu,\n      logout\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","useStore","useRouter","setup","store","router","isAuthenticated","getters","showSideMenu","toggleSideMenu","value","sideMenuElement","document","querySelector","classList","add","remove","logout","dispatch","push"],"sources":["C:\\FTN\\treca\\drugi_sem\\veb\\chocolate-factoryapp\\Frontend\\front-app\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <nav>\r\n      <router-link to=\"/\">Home</router-link> |\r\n      <router-link to=\"/factories\">Factories</router-link> |\r\n      <router-link to=\"/add-factory\">Add Factory</router-link> |\r\n      <router-link to=\"/cart\">Cart</router-link> |\r\n      <button @click=\"toggleSideMenu\">Menu</button>\r\n    </nav>\r\n    <div v-if=\"showSideMenu\" class=\"side-menu\">\r\n      <button @click=\"toggleSideMenu\" class=\"close-btn\">&times;</button>\r\n      <router-link v-if=\"!isAuthenticated\" to=\"/login\" @click=\"toggleSideMenu\">Login</router-link>\r\n      <router-link v-if=\"!isAuthenticated\" to=\"/register\" @click=\"toggleSideMenu\">Register</router-link>\r\n      <button v-if=\"isAuthenticated\" @click=\"logout\">Logout</button>\r\n    </div>\r\n    <router-view/>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const isAuthenticated = computed(() => store.getters.isAuthenticated);\r\n    const showSideMenu = ref(false);\r\n\r\n    const toggleSideMenu = () => {\r\n      showSideMenu.value = !showSideMenu.value;\r\n      const sideMenuElement = document.querySelector('.side-menu');\r\n      if (showSideMenu.value) {\r\n        sideMenuElement.classList.add('show');\r\n      } else {\r\n        sideMenuElement.classList.remove('show');\r\n      }\r\n    };\r\n\r\n    const logout = () => {\r\n      store.dispatch('logout');\r\n      router.push('/login');\r\n      toggleSideMenu();\r\n    };\r\n\r\n    return {\r\n      isAuthenticated,\r\n      showSideMenu,\r\n      toggleSideMenu,\r\n      logout\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: 'Arial', sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  background-color: #D2B48C;\r\n  min-height: 100vh;\r\n}\r\n\r\nnav {\r\n  padding: 30px;\r\n}\r\n\r\nnav a,\r\nnav button {\r\n  font-weight: bold;\r\n  color: #2c3e50;\r\n  text-decoration: none;\r\n  padding: 0 15px;\r\n  font-size: 1.2em;\r\n  transition: color 0.3s;\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\nnav a.router-link-exact-active {\r\n  color: #4b3832;\r\n}\r\n\r\nnav a:hover,\r\nnav button:hover {\r\n  color: #FFD700;\r\n}\r\n\r\n.side-menu {\r\n  position: fixed;\r\n  right: 0;\r\n  top: 0;\r\n  height: 100%;\r\n  width: 250px;\r\n  background-color: #4b3832;\r\n  box-shadow: -2px 0 5px rgba(0,0,0,0.5);\r\n  padding-top: 60px;\r\n  transition: transform 0.3s ease-in-out;\r\n  transform: translateX(100%);\r\n  z-index: 1000;\r\n}\r\n\r\n.side-menu.show {\r\n  transform: translateX(0);\r\n}\r\n\r\n.side-menu a,\r\n.side-menu button {\r\n  display: block;\r\n  color: #FFD700;\r\n  padding: 16px;\r\n  text-decoration: none;\r\n  transition: color 0.3s;\r\n}\r\n\r\n.side-menu a:hover,\r\n.side-menu button:hover {\r\n  color: #ffffff;\r\n}\r\n\r\n.close-btn {\r\n  position: absolute;\r\n  top: 20px;\r\n  right: 25px;\r\n  font-size: 36px;\r\n  color: #FFD700;\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAoBA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAK,GAAIH,SAAS,CAAC,CAAC;IAC1B,MAAMI,eAAc,GAAIP,QAAQ,CAAC,MAAMK,KAAK,CAACG,OAAO,CAACD,eAAe,CAAC;IACrE,MAAME,YAAW,GAAIR,GAAG,CAAC,KAAK,CAAC;IAE/B,MAAMS,cAAa,GAAIA,CAAA,KAAM;MAC3BD,YAAY,CAACE,KAAI,GAAI,CAACF,YAAY,CAACE,KAAK;MACxC,MAAMC,eAAc,GAAIC,QAAQ,CAACC,aAAa,CAAC,YAAY,CAAC;MAC5D,IAAIL,YAAY,CAACE,KAAK,EAAE;QACtBC,eAAe,CAACG,SAAS,CAACC,GAAG,CAAC,MAAM,CAAC;MACvC,OAAO;QACLJ,eAAe,CAACG,SAAS,CAACE,MAAM,CAAC,MAAM,CAAC;MAC1C;IACF,CAAC;IAED,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBb,KAAK,CAACc,QAAQ,CAAC,QAAQ,CAAC;MACxBb,MAAM,CAACc,IAAI,CAAC,QAAQ,CAAC;MACrBV,cAAc,CAAC,CAAC;IAClB,CAAC;IAED,OAAO;MACLH,eAAe;MACfE,YAAY;MACZC,cAAc;MACdQ;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}