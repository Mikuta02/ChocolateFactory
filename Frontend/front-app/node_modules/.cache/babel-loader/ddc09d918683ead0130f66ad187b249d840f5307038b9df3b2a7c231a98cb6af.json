{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  data() {\n    return {\n      text: '',\n      rating: 5,\n      factoryId: null // Inicijalizujemo kao null\n    };\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute(); // Dodajemo useRoute za pristup parametrima rute\n\n    // Postavljamo factoryId iz ruta parametara kada se komponenta montira\n    if (route.params.factoryId) {\n      this.factoryId = Number(route.params.factoryId); // Konvertujemo u broj\n    }\n    return {\n      store,\n      router,\n      route\n    };\n  },\n  methods: {\n    submitComment() {\n      const userId = this.store.getters.userId;\n      const token = this.store.state.token;\n      if (!userId) {\n        console.error('User is not defined or user.id is missing');\n        return;\n      }\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId: this.factoryId,\n        // Koristimo this.factoryId\n        text: this.text,\n        rating: this.rating\n      }, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log('Comment added:', response.data);\n        this.router.push('/purchases');\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["useStore","axios","useRouter","useRoute","data","text","rating","factoryId","setup","store","router","route","params","Number","methods","submitComment","userId","getters","token","state","console","error","post","headers","then","response","log","push","catch"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"text\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <input id=\"rating\" type=\"number\" v-model=\"rating\" min=\"1\" max=\"5\" required />\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useStore } from 'vuex';\r\nimport axios from 'axios';\r\nimport { useRouter, useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      text: '',\r\n      rating: 5,\r\n      factoryId: null,  // Inicijalizujemo kao null\r\n    };\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const route = useRoute();  // Dodajemo useRoute za pristup parametrima rute\r\n\r\n    // Postavljamo factoryId iz ruta parametara kada se komponenta montira\r\n    if (route.params.factoryId) {\r\n      this.factoryId = Number(route.params.factoryId);  // Konvertujemo u broj\r\n    }\r\n\r\n    return { store, router, route };\r\n  },\r\n  methods: {\r\n    submitComment() {\r\n      const userId = this.store.getters.userId;\r\n      const token = this.store.state.token;\r\n\r\n      if (!userId) {\r\n        console.error('User is not defined or user.id is missing');\r\n        return;\r\n      }\r\n\r\n      axios.post('http://localhost:3001/api/comments', {\r\n        userId,\r\n        factoryId: this.factoryId,  // Koristimo this.factoryId\r\n        text: this.text,\r\n        rating: this.rating,\r\n      }, {\r\n        headers: {\r\n          'Authorization': `Bearer ${token}`\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Comment added:', response.data);\r\n        this.router.push('/purchases');\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding comment:', error);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilizacija */\r\n</style>\r\n"],"mappings":";AAkBA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAEhD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,CAAC;MACTC,SAAS,EAAE,IAAI,CAAG;IACpB,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIT,QAAQ,CAAC,CAAC;IACxB,MAAMU,MAAK,GAAIR,SAAS,CAAC,CAAC;IAC1B,MAAMS,KAAI,GAAIR,QAAQ,CAAC,CAAC,EAAG;;IAE3B;IACA,IAAIQ,KAAK,CAACC,MAAM,CAACL,SAAS,EAAE;MAC1B,IAAI,CAACA,SAAQ,GAAIM,MAAM,CAACF,KAAK,CAACC,MAAM,CAACL,SAAS,CAAC,EAAG;IACpD;IAEA,OAAO;MAAEE,KAAK;MAAEC,MAAM;MAAEC;IAAM,CAAC;EACjC,CAAC;EACDG,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,MAAMC,MAAK,GAAI,IAAI,CAACP,KAAK,CAACQ,OAAO,CAACD,MAAM;MACxC,MAAME,KAAI,GAAI,IAAI,CAACT,KAAK,CAACU,KAAK,CAACD,KAAK;MAEpC,IAAI,CAACF,MAAM,EAAE;QACXI,OAAO,CAACC,KAAK,CAAC,2CAA2C,CAAC;QAC1D;MACF;MAEApB,KAAK,CAACqB,IAAI,CAAC,oCAAoC,EAAE;QAC/CN,MAAM;QACNT,SAAS,EAAE,IAAI,CAACA,SAAS;QAAG;QAC5BF,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,MAAM,EAAE,IAAI,CAACA;MACf,CAAC,EAAE;QACDiB,OAAO,EAAE;UACP,eAAe,EAAG,UAASL,KAAM;QACnC;MACF,CAAC,EACAM,IAAI,CAACC,QAAO,IAAK;QAChBL,OAAO,CAACM,GAAG,CAAC,gBAAgB,EAAED,QAAQ,CAACrB,IAAI,CAAC;QAC5C,IAAI,CAACM,MAAM,CAACiB,IAAI,CAAC,YAAY,CAAC;MAChC,CAAC,EACAC,KAAK,CAACP,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}