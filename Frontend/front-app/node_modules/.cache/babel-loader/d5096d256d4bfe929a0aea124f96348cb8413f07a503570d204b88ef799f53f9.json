{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  data() {\n    return {\n      text: '',\n      rating: 5,\n      factoryId: null // Definišemo factoryId kao null\n    };\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute();\n    return {\n      store,\n      router,\n      route\n    };\n  },\n  mounted() {\n    this.factoryId = parseInt(this.$route.params.factoryId, 10); // Konvertujemo factoryId u broj\n  },\n  methods: {\n    submitComment() {\n      const userId = this.store.getters.userId;\n      const token = this.store.state.token;\n      if (!userId || !this.factoryId) {\n        console.error('User ID or Factory ID is missing');\n        return;\n      }\n      console.log(`Submitting comment with factoryId: ${this.factoryId}`);\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId: this.factoryId,\n        text: this.text,\n        rating: this.rating\n      }, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log('Comment added:', response.data);\n        this.router.push('/purchases');\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["useStore","axios","useRouter","useRoute","data","text","rating","factoryId","setup","store","router","route","mounted","parseInt","$route","params","methods","submitComment","userId","getters","token","state","console","error","log","post","headers","then","response","push","catch"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"text\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <input id=\"rating\" type=\"number\" v-model=\"rating\" min=\"1\" max=\"5\" required />\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useStore } from 'vuex';\r\nimport axios from 'axios';\r\nimport { useRouter, useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      text: '',\r\n      rating: 5,\r\n      factoryId: null, // Definišemo factoryId kao null\r\n    };\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    return { store, router, route };\r\n  },\r\n  mounted() {\r\n    this.factoryId = parseInt(this.$route.params.factoryId, 10); // Konvertujemo factoryId u broj\r\n  },\r\n  methods: {\r\n    submitComment() {\r\n      const userId = this.store.getters.userId;\r\n      const token = this.store.state.token;\r\n\r\n      if (!userId || !this.factoryId) {\r\n        console.error('User ID or Factory ID is missing');\r\n        return;\r\n      }\r\n\r\n      console.log(`Submitting comment with factoryId: ${this.factoryId}`);\r\n\r\n      axios.post('http://localhost:3001/api/comments', {\r\n        userId,\r\n        factoryId: this.factoryId,\r\n        text: this.text,\r\n        rating: this.rating,\r\n      }, {\r\n        headers: {\r\n          'Authorization': `Bearer ${token}`\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Comment added:', response.data);\r\n        this.router.push('/purchases');\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding comment:', error);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilizacija */\r\n</style>\r