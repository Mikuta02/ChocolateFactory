/Programiranje/veb/projekat/chocolate-factoryapp/Frontend/front-app/src/components/AddComment.vue"],"sourcesContent":["<template>\r\n  <div class=\"add-comment\">\r\n    <h1>Add Comment</h1>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"commentText\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <select id=\"rating\" v-model=\"commentRating\" required>\r\n          <option value=\"1\">1</option>\r\n          <option value=\"2\">2</option>\r\n          <option value=\"3\">3</option>\r\n          <option value=\"4\">4</option>\r\n          <option value=\"5\">5</option>\r\n        </select>\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport axios from 'axios';\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport { useStore } from 'vuex';\r\n\r\nconst route = useRoute();\r\nconst router = useRouter();\r\nconst store = useStore();\r\nconst commentText = ref('');\r\nconst commentRating = ref(5);\r\n\r\nconst submitComment = () => {\r\n  const factoryId = route.params.factoryId;\r\n  const userId = store.state.userId;\r\n\r\n  axios.post('http://localhost:3001/api/comments', {\r\n    userId,\r\n    factoryId,\r\n    text: commentText.value,\r\n    rating: commentRating.value\r\n  }, {\r\n    headers: {\r\n      'Authorization': `Bearer ${store.state.token}`\r\n    }\r\n  })\r\n  .then(response => {\r\n    console.log('Comment submitted:', response.data);\r\n    router.push('/purchases'); // Preusmeravanje nazad na listu kupovina\r\n  })\r\n  .catch(error => {\r\n    console.error('Error submitting comment:', error);\r\n  });\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.add-comment {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f5f5f5;