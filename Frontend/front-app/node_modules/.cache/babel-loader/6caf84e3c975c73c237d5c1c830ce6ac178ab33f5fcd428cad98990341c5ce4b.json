{"ast":null,"code":"import { useStore } from 'vuex';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      text: '',\n      rating: 5,\n      factoryId: 1 // Pretpostavljamo da imate factoryId iz nekog izvora\n    };\n  },\n  setup() {\n    const store = useStore();\n    return {\n      store\n    };\n  },\n  methods: {\n    submitComment() {\n      const userId = this.store.state.user.id; // Uzimanje userId iz Vuex store-a\n      axios.post('http://localhost:3001/api/comments', {\n        userId,\n        factoryId: this.factoryId,\n        text: this.text,\n        rating: this.rating\n      }).then(response => {\n        console.log('Comment added:', response.data);\n      }).catch(error => {\n        console.error('Error adding comment:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["useStore","axios","data","text","rating","factoryId","setup","store","methods","submitComment","userId","state","user","id","post","then","response","console","log","catch","error"],"sources":["C:\\Programiranje\\veb\\projekat\\chocolate-factoryapp\\Frontend\\front-app\\src\\components\\AddComment.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Comment</h2>\r\n    <form @submit.prevent=\"submitComment\">\r\n      <div>\r\n        <label for=\"text\">Comment:</label>\r\n        <textarea id=\"text\" v-model=\"text\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"rating\">Rating:</label>\r\n        <input id=\"rating\" type=\"number\" v-model=\"rating\" min=\"1\" max=\"5\" required />\r\n      </div>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useStore } from 'vuex';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      text: '',\r\n      rating: 5,\r\n      factoryId: 1, // Pretpostavljamo da imate factoryId iz nekog izvora\r\n    };\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    return { store };\r\n  },\r\n  methods: {\r\n    submitComment() {\r\n      const userId = this.store.state.user.id; // Uzimanje userId iz Vuex store-a\r\n      axios.post('http://localhost:3001/api/comments', {\r\n        userId,\r\n        factoryId: this.factoryId,\r\n        text: this.text,\r\n        rating: this.rating,\r\n      })\r\n      .then(response => {\r\n        console.log('Comment added:', response.data);\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding comment:', error);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAkBA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,CAAC;MACTC,SAAS,EAAE,CAAC,CAAE;IAChB,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIP,QAAQ,CAAC,CAAC;IACxB,OAAO;MAAEO;IAAM,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,MAAMC,MAAK,GAAI,IAAI,CAACH,KAAK,CAACI,KAAK,CAACC,IAAI,CAACC,EAAE,EAAE;MACzCZ,KAAK,CAACa,IAAI,CAAC,oCAAoC,EAAE;QAC/CJ,MAAM;QACNL,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBF,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,MAAM,EAAE,IAAI,CAACA;MACf,CAAC,EACAW,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ,CAACd,IAAI,CAAC;MAC9C,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}